#!/usr/bin/perl -wT

use strict;
use Time::Local;
use List::Util qw (max min);
use POSIX;

#------------------------------------------------------------------------------
# Edit these (watch your formatting) to set your day start and end times
my $workdayStartTime     = "8:00:00";
my $workdayStartMeridium = "AM";

my $workdayEndTime     = "5:00:00";
my $workdayEndMeridium = "PM";

###############################################################################
# NO USER SERVICEABLE PARTS BELOW!
# DO NOT EDIT PAST THIS POINT
#------------------------------------------------------------------------------

# Constant: Seconds in a day
my $SECSPERDAY = 86400;

# Constant: Month and weekday names
my @monthName = qw(Jan Feb Mar Apr May Jun Jul Aug Sep Oct Nov Dec);
my @wdayName  = qw(Sun Mon Tue Wed Thu Fri Sat);

# Constant: get the time zone from the system configuration.
my $timeZone = strftime("%Z", localtime());

# Global hash used as sparse array to store schedule
my %schedule;

#------------------------------------------------------------------------------

# Convert an event (passed as $date, $time, $meridium) to epoch time
sub event2Epoch ($$$) {

    # Expected argument format:
    #   ARG1: 7/2/14 (Month, Day, Year)
    #   ARG2: 2:30:00 (12-hour, Minute, Second)
    #   ARG3: PM if afternoon

    my @dateArray = split( '/', shift );
    my @timeArray = split( ':', shift );
    my $meridium  = shift;

    # Convert 12-hour to 24-hour time
    $timeArray[0] = convert12hto24h($timeArray[0], $meridium);

   # Pass elements to timelocal, modifying for counting months at 0 instead of 1
    return
      timelocal( $timeArray[2], $timeArray[1],     $timeArray[0],
                 $dateArray[1], $dateArray[0] - 1, $dateArray[2] );
}

#------------------------------------------------------------------------------

# Parse events passed from STDIN
sub parseEvents () {
    my (
         $startTime,   $startMeridium, $endTime,
         $endMeridium, $startDate,     $endDate
    );
    my $inHeader = 0;    # Boolean: are we in the event header?

    # Determine

    # Read events from STDIN
    foreach my $line (&lt;STDIN&gt;) {
        chop $line;

        # Parse each line based on the starting word
        # Use "isHeader" to store if we're looking at header or note information
        if ( ( $line =~ /^EVENT/ ) &amp;&amp; ( !$inHeader ) ) {
            ( $startTime, $endTime, $startDate, $endDate ) = undef;
            $inHeader = 1;
        }
        elsif ( ( $line =~ /^Date:/ ) &amp;&amp; $inHeader ) {
            ( undef, $startDate, undef, $endDate ) = split( ' ', $line );
        }
        elsif ( ( $line =~ /^Time:/ ) &amp;&amp; $inHeader ) {
            ( undef, $startTime, $startMeridium, undef, $endTime, $endMeridium )
              = split( ' ', $line );
        }
        elsif ( ( ( $line =~ /^Notes:/ ) || ( $line =~ /^$/ ) ) &amp;&amp; $inHeader ) {
            $inHeader = 0;

       # Convert the record into seconds since epoch and insert it into the hash
            $schedule{ event2Epoch( $startDate, $startTime, $startMeridium ) }
              = '+';
            $schedule{ event2Epoch( $endDate, $endTime, $endMeridium ) } = '-';
        }
    }
}

#------------------------------------------------------------------------------
# Given an epoch, return the date as a string sufficient for event2epoch fxn
sub getDateString ($) {
    my ( undef, undef, undef, $mday, $mon, $year ) = localtime(shift);
    $mon++;    # We count months starting with 1, not zero
    $year += 1900;    # Years since 1900

    return "$mon/$mday/$year";
}

#------------------------------------------------------------------------------
# Iterate through the schedule, adding start/end times for each date
sub addWorkDaySchedule () {

    # Find the start and end dates
    my $startStamp = min( keys %schedule );    # Reuse this as a counter
    my $endStamp   = max( keys %schedule );

    # Iterate through those dates, inserting records for workday start/end
    while ( $startStamp &lt; $endStamp ) {

        # Insert a day start and end to ensure we have the right records
        $schedule{
            event2Epoch(
                         getDateString($startStamp), $workdayStartTime,
                         $workdayStartMeridium
            )
        } = 's';
        $schedule{
            event2Epoch(
                         getDateString($startStamp), $workdayEndTime,
                         $workdayEndMeridium
            )
        } = 'e';

        $startStamp += $SECSPERDAY;
    }
}

#------------------------------------------------------------------------------
# Convert 24 hour to 12 hour time
sub convert24hto12h ($) {
    my $hour = shift;

    if ( ( $hour &lt; 0 ) || ( $hour &gt; 23 ) ) {
        die("$hour is not a valid hour");
    }

    return ( $hour, "AM" ) if $hour &lt; 12;
    return ( $hour, "PM" ) if $hour == 12;
    return ( $hour - 12, "PM" );
}

#------------------------------------------------------------------------------
# Convert 12 hour time to 24 hour time
sub convert12hto24h ($$) {
  my $hour = shift;
  my $meridian = shift;

  if ( ( $hour &lt; 0 ) || ( $hour &gt; 23 ) ) {
    die("$hour is not a valid hour");
  }

  $hour -= 12 if (($meridian =~ /^AM$/i) &amp;&amp; ($hour == 12));
  $hour += 12 if (($meridian =~ /^PM$/i) &amp;&amp; ($hour != 12));

  return $hour;
}

#------------------------------------------------------------------------------
# Convert raw seconds to Hours:Minutes:Seconds
sub secsToHMS ($) {
  my $time = shift;
  my $days = int($time / 86400);
  $time -= ($days * 86400);
  my $hours = int($time / 3600);
  $time -= ($hours * 3600);
  my $minutes = int($time / 60);
  my $seconds = $time % 60;

  $days = $days &lt; 1 ? '' : $days .'d ';
  $hours = $hours &lt; 1 ? '' : $hours .'h ';
  $minutes = $minutes &lt; 1 ? '' : $minutes . 'm ';
  $time = $days . $hours . $minutes . $seconds . 's';
  return $time;
}

#------------------------------------------------------------------------------
# Print free times as provided
sub printFreeTime ($$) {
    my $startTime = shift;
    my $endTime = shift;
    my $diffTime = secsToHMS($endTime - $startTime);

    my ( undef, $smin, $shour, $smday, $smon, $syear, $swday, $syday, $sisdst )
      = localtime($startTime);
    my ( undef, $emin, $ehour, $emday, $emon, $eyear, $ewday, $eyday, $eisdst )
      = localtime($endTime);

    # Convert 24 to 12 hour
    my ( $sMeridian, $eMeridian ) = ( "XX", "XX" );
    ( $shour, $sMeridian ) = convert24hto12h($shour);
    ( $ehour, $eMeridian ) = convert24hto12h($ehour);

    # Handle pretty-printing, padding, and formatting
    $smday = sprintf( "%2s", $smday );
    $syear += 1900;    # Fix years since 1900
    $shour = sprintf( "%2s",  $shour );
    $smin  = sprintf( "%02d", $smin );
    $ehour = sprintf( "%2s",  $ehour );
    $emin  = sprintf( "%02d", $emin );

    print
"- $wdayName[$swday] $monthName[$smon] $smday $syear: $shour:$smin $sMeridian -&gt; $ehour:$emin $eMeridian $timeZone ($diffTime)\n";
}

#------------------------------------------------------------------------------
# Iterate through the schedule, printing open times
sub findFreeTime () {
    my $depthCount = 0;    # How many items are we actively working?
    my $lastEvent  = 0;    # Whats the date/time epoch of the last event?
    my $isWorkDay  = 0;    # Are we in a work day?

    foreach my $event ( sort keys %schedule ) {
        #print "-"
        #  . "-" x $depthCount . " "
        #  . localtime($event) . " "
        #  . $schedule{$event} . " ("
        #  . $depthCount . ")\n";
        if ( ( $depthCount == 1 ) &amp;&amp; ( $lastEvent &gt; 0 ) &amp;&amp; $isWorkDay ) {
            printFreeTime( $lastEvent, $event );
        }

        # Determine how to handle the event in the hash
        if ( $schedule{$event} eq '+' ) {
            $depthCount++;
        }
        elsif ( $schedule{$event} eq '-' ) {
            $depthCount--;
        }
        elsif ( $schedule{$event} eq 's' ) {
            $isWorkDay = $depthCount = 1;
        }
        elsif ( $schedule{$event} eq 'e' ) {
            $isWorkDay = $depthCount = 0;
        }
        else {
            warn "Unknown event: \"$event\" = \"$schedule{$event}\"\n";
        }

        $lastEvent = $event;
    }
}

#------------------------------------------------------------------------------
# Main

parseEvents();
addWorkDaySchedule();
findFreeTime();
